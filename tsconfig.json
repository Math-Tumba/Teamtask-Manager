{
    "compilerOptions": {
        "baseUrl": "./assets/",
        "target": "es2020",
        "lib": ["es2020", "dom", "dom.iterable"],
        "moduleResolution": "node",
        "esModuleInterop": true,
        "useUnknownInCatchVariables": false,
        "paths": {
            "@symfony/ux-live-component": ["../vendor/symfony/ux-live-component/assets/dist/live_controller.js"],
            "ky": ["vendor/ky/ky.index.js"],
            "api": ["ts/utils/kyManager.ts"],
            "components/*": ["ts/components/*"],
            "utils/*": ["ts/utils/*"],
            "ajax/*": ["ts/ajax/*"]
        },
    }
}